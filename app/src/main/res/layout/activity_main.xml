<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_normal_notification"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="normal_notification" />

    <Button
        android:id="@+id/btn_set_channel_vibrate_and_sound"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="给channel设置振动和声音"/>

    <Button
        android:id="@+id/btn_set_channel_silent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="给channel设置静默"/>

</LinearLayout>